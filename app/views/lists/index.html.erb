 <h1 class="mx-auto" style="width: 200px"> My lists </h1>

<ul class="list-group">
    <% @lists.each do |list| %>
      <li class="list-group-item">
        <%= link_to "#{list.name}", list_path(list) %>
      </li>
    <% end %>
 </ul>

   <div class= "btn btn-success">
      <%= link_to "Add a list", new_list_path %>
  </div>
